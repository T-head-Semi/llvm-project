bool isUImm2() const {
  int64_t Imm;
  RISCVMCExpr::VariantKind VK = RISCVMCExpr::VK_RISCV_None;
  if (!isImm())
    return false;
  bool IsConstantImm = evaluateConstantImm(getImm(), Imm, VK);
  return IsConstantImm && isUInt<2>(Imm) && VK == RISCVMCExpr::VK_RISCV_None;
}

